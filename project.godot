; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot Rollback Netcode Demo"
run/main_scene="res://rollback_netcode_demo/Main.tscn"
config/features=PackedStringArray("4.0")

[autoload]

SyncDebugger="*res://addons/godot-rollback-netcode/SyncDebugger.gd"
SyncManager="*res://addons/godot-rollback-netcode/SyncManager.gd"
SyncReplay="*res://addons/godot-rollback-netcode/SyncReplay.gd"

[debug]

multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="server"
multirun/other_window_args="client"

[display]

window/stretch/mode="2d"

[editor]

export/convert_text_resources_to_binary=true

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-rollback-netcode/plugin.cfg")

